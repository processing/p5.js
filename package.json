{
  "name": "p5",
  "repository": "processing/p5.js",
  "scripts": {
    "build": "rollup -c",
    "preview": "vite",
    "dev": "grunt yui browserify:dev connect:yui watch:quick",
    "docs": "documentation build ./src/**/*.js ./src/**/**/*.js -o ./docs/data.json && node ./utils/convert.js",
    "test": "vitest run",
    "lint": "eslint .",
    "lint:fix": "eslint --fix ."
  },
  "lint-staged": {
    "Gruntfile.js": "eslint",
    "docs/preprocessor.js": "eslint",
    "utils/**/*.js": "eslint",
    "tasks/**/*.js": "eslint",
    "src/**/*.js": [
      "eslint",
      "node --require @babel/register ./utils/sample-linter.js"
    ]
  },
  "version": "1.9.1-1",
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.10.2",
    "@babel/register": "^7.7.7",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@vitest/browser": "^0.34.6",
    "all-contributors-cli": "^6.19.0",
    "babel-plugin-i18next-extract": "^0.5.0",
    "babel-plugin-istanbul": "^5.2.0",
    "babelify": "^10.0.0",
    "brfs-babel": "^2.0.0",
    "browserify": "^16.5.0",
    "connect-modrewrite": "^0.10.1",
    "core-js": "^3.6.5",
    "dayjs": "^1.11.10",
    "derequire": "^2.0.0",
    "documentation": "^14.0.3",
    "eslint": "^8.54.0",
    "file-saver": "^1.3.8",
    "gifenc": "^1.0.3",
    "grunt": "^1.6.1",
    "grunt-cli": "^1.4.3",
    "grunt-contrib-connect": "^3.0.0",
    "grunt-contrib-uglify": "^5.2.2",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-eslint": "^24.0.0",
    "grunt-minjson": "^0.4.0",
    "grunt-mocha-test": "^0.13.3",
    "grunt-newer": "^1.3.0",
    "grunt-simple-nyc": "^3.0.1",
    "husky": "^4.2.3",
    "i18next": "^19.0.2",
    "i18next-browser-languagedetector": "^4.0.1",
    "libtess": "^1.2.2",
    "lint-staged": "^15.1.0",
    "mocha": "^10.2.0",
    "omggif": "^1.0.10",
    "opentype.js": "^1.3.1",
    "pretty-fast": "^0.2.7",
    "promise-map-series": "^0.2.3",
    "puppeteer": "^18.2.1",
    "regenerator-runtime": "^0.13.3",
    "rollup": "^4.9.6",
    "rollup-plugin-string": "^3.0.0",
    "unplugin-swc": "^1.4.2",
    "vite": "^5.0.2",
    "vite-plugin-string": "^1.2.2",
    "vitest": "^0.34.6",
    "webdriverio": "^8.24.1"
  },
  "license": "LGPL-2.1",
  "main": "./lib/p5.min.js",
  "files": [
    "license.txt",
    "lib/p5.min.js",
    "lib/p5.js",
    "lib/addons/p5.sound.js",
    "lib/addons/p5.sound.min.js",
    "translations/**"
  ],
  "description": "[![npm version](https://badge.fury.io/js/p5.svg)](https://www.npmjs.com/package/p5)",
  "bugs": {
    "url": "https://github.com/processing/p5.js/issues"
  },
  "homepage": "https://p5js.org",
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "usage",
          "corejs": 3
        }
      ]
    ],
    "plugins": [
      [
        "i18next-extract",
        {
          "locales": [
            "en",
            "es"
          ],
          "outputPath": "translations/{{locale}}/{{ns}}.json",
          "tFunctionNames": [
            "translator"
          ]
        }
      ]
    ],
    "env": {
      "test": {
        "plugins": [
          [
            "istanbul",
            {
              "include": [
                "src/**/*.js"
              ]
            }
          ]
        ]
      }
    }
  },
  "browserslist": [
    "last 2 versions",
    "not dead"
  ],
  "author": "",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
